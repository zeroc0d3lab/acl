{
    "name": "zeroc0d3lab/acl",
    "description": "Laravel acl management",
    "license": "MIT",
    "keywords": [
        "zeroc0d3lab",
        "zeroc0d3lab cms",
        "zeroc0d3lab platform",
        "zeroc0d3lab acl"
    ],
    "homepage": "https://zeroc0d3lab.com",
    "authors": [
        {
            "name": "ZeroC0D3 Team",
            "email": "zeroc0d3.team@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "^5.6|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0|^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Zeroc0d3lab\\ACL\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zeroc0d3lab\\ACL\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zeroc0d3lab\\ACL\\Providers\\AclServiceProvider"
            ],
            "aliases": {
                "ACL": "Zeroc0d3lab\\ACL\\Facades\\AclFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
